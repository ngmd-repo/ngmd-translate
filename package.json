{
  "name": "ngmd-translate",
  "version": "0.0.1",
  "scripts": {
    "-------APP-------": "",
    "app:dev": "ng serve",
    "ng": "ng",
    "start": "concurrently \"npm run lib:dev\" \"npm run app:dev\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky",
    "-------LIBRARY-------": "",
    "lib:dev": "ng build translate --configuration=development",
    "lib:build": "ng build translate --configuration production",
    "-------DOCS-------": "",
    "docs:dev": "ng s docs",
    "docs:build": "ng b docs --clear-screen=true",
    "docs:en:dev": "ng s docs-en",
    "docs:en:build": "ng b docs-en --clear-screen=true",
    "docs:all:build": "npm run docs:build && npm run docs:en:build",
    "-------FIREBASE-------": "",
    "firebase:preview": "firebase emulators:start",
    "firebase:deploy": "npm run docs:build && firebase deploy --only hosting:docs",
    "firebase:en:deploy": "npm run docs:en:build && firebase deploy --only hosting:docs-en",
    "firebase:deploy:all": "npm run docs:build && npm run docs:en:build && firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.4",
    "@angular/cdk": "^20.2.2",
    "@angular/common": "^20.2.4",
    "@angular/compiler": "^20.2.4",
    "@angular/core": "^20.2.4",
    "@angular/forms": "^20.2.4",
    "@angular/platform-browser": "^20.2.4",
    "@angular/platform-browser-dynamic": "^20.2.4",
    "@angular/router": "^20.2.4",
    "@jsverse/transloco": "^8.0.0",
    "@jsverse/transloco-messageformat": "^8.0.0",
    "@ng-doc/app": "~20.1.0",
    "@ng-doc/builder": "~20.1.0",
    "@ng-doc/core": "~20.1.0",
    "@ng-doc/keywords-loaders": "~20.1.0",
    "@ng-doc/ui-kit": "~20.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.2.2",
    "@angular/build": "^20.2.2",
    "@angular/cli": "~20.2.2",
    "@angular/compiler-cli": "^20.2.4",
    "@ngmd/git-flow": "~0.0.3",
    "@ngmd/linter": "~0.0.1",
    "@types/jasmine": "~4.3.0",
    "concurrently": "9.0.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.0.0",
    "typescript": "~5.8.3"
  }
}
